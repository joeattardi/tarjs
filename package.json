{
  "name": "tarjs-cli",
  "version": "0.3.1",
  "description": "Command-line tar utility",
  "bin": {
    "tarjs": "src/index.js"
  },
  "scripts": {
    "lint": "eslint src",
    "prettify": "prettier --write src/**/*js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joeattardi/tarjs.git"
  },
  "keywords": [
    "tar",
    "cli"
  ],
  "author": "Joe Attardi",
  "license": "MIT",
  "engines": {
    "node": ">8.12.0"
  },
  "bugs": {
    "url": "https://github.com/joeattardi/tarjs/issues"
  },
  "homepage": "https://npmjs.com/package/tarjs-cli",
  "dependencies": {
    "chalk": "^2.4.1",
    "commander": "^2.19.0",
    "debug": "^4.1.0",
    "filesize": "^3.6.1",
    "ora": "^3.0.0",
    "tar": "^4.4.6"
  },
  "devDependencies": {
    "eslint": "^5.8.0",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.1.2",
    "lint-staged": "^8.0.4",
    "prettier": "^1.14.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --write",
      "git add"
    ]
  }
}
